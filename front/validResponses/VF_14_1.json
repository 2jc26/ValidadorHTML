{
  "htmlStructure": {
    "node": "root",
    "children": [
      {
        "tag": "html",
        "children": [
          {
            "tag": "head",
            "children": [
              {
                "tag": "title"
              },
              {
                "tag": "link",
                "attr": {
                  "rel": "stylesheet",
                  "href": "style.css"
                }
              }
            ]
          },
          {
            "tag": "body",
            "children": [
              {
                "tag": "h1"
              },
              {
                "tag": "p",
                "attr": {
                  "class": "primero"
                }
              },
              {
                "tag": "p",
                "attr": {
                  "class": "segundo"
                }
              },
              {
                "tag": "p",
                "attr": {
                  "class": "tercero"
                }
              }
            ]
          }
        ]
      }
    ]
  },
  "cssStructure": {
    ".primero": {
      "font-style": "italic"
    },
    ".segundo": {
      "font-weight": "bold"
    },
    ".tercero": {
      "font-size": "20px",
      "font-family": "'Courier New', Courier, monospace"
    }
  }
}
